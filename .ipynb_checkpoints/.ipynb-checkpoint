    }
   ],
   "source": [
    "outlier_z = []\n",
    "mean = np.mean(df['ratings_count'])\n",
    "std = np.std(df['ratings_count'])\n",
    "mean, std\n",
    "treshold = 3\n",
    "\n",
    "for i in df['ratings_count']:\n",
    "    z_score = (i - mean) / std\n",
    "    if np.abs(z_score) > treshold:\n",
    "        outlier_z.append(i)\n",
    "\n",
    "       \n",
    "df_z = df.loc[~df['ratings_count'].isin(outlier_z)]\n",
    "df_z"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 1440x360 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Graphic without IRQ outliers\n",
    "plt.figure(figsize=(20,5))\n",
    "plt.hist(df_no_outlier['ratings_count'])\n",
    "plt.title('Count of Ratings')\n",
    "plt.show()"
   ]
